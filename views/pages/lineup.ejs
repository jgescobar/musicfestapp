<!DOCTYPE html>
<html>
<head>
  <%- include('../partials/head') %>
</head>
<body>

<%- include('../partials/header') %>

  <main>
  <a class="modal-trigger waves-effect waves-light btn" id="artistbutton" href="#modal1">Pick Your Artist</a>

<div id="line-up">
</div>

  <!-- Modal Structure -->

  <div id="modal1" class="modal modal-fixed-footer">
    <div class="modal-content">
      <h4>Add an artist to your Dream Line Up</h4>

       <form id="search-artist">
       <div class="row">
        <div class="input-field col s12">
          <input id="artist" type="text">
          <label for="artist">Artist Name</label>
        </div>
      </div>
        <button class="btn waves-effect waves-light" type="submit" name="action">Submit<i class="material-icons right">send</i>
        </button>
      </form>

      <ul id="artist-list">

      </ul>

    </div>
    <div class="modal-footer">
      <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat ">Back to my lineup
      <i class="small material-icons">done</i>
      </a>
    </div>
  </div>
  </main>

  <%- include('../partials/scripts') %>
</body>
</html>
